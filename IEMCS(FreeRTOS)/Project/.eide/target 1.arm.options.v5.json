{
    "version": 2,
    "beforeBuildTasks": [],
    "afterBuildTasks": [
        {
            "name": "clean",
            "command": "cd \"${OutDir}\" && del *._*"
        }
    ],
    "global": {
        "use-microLIB": true,
        "output-debug-info": "enable"
    },
    "c/cpp-compiler": {
        "optimization": "level-0",
        "one-elf-section-per-function": true,
        "c99-mode": true,
        "misc-controls": "--diag_suppress=1 --diag_suppress=1295"
    },
    "asm-compiler": {},
    "linker": {
        "output-lib": false
    }
}